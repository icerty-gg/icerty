{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Client",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "ES2021",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"module": "esnext",
		"allowJs": true,
		"noEmit": true,
		"jsx": "preserve",
		"incremental": true,
		"resolveJsonModule": true
	}
}
