{
	"name": "server",
	"version": "1.0.0",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "prisma generate && prisma db seed && nodemon index.ts",
		"start": "node dist/index.js",
		"lint": "eslint .",
